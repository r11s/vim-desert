{
  "$schema": "vscode://schemas/color-theme",
  "name": "Desert (Vim port No Bold)",
  "type": "dark",
  "colors": {
    "foreground": "#ffffff",
    "focusBorder": "#c2bfa5",
    "selection.background": "#6b8e24",

    "editor.background": "#333333",
    "editor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#666666",
    "editorLineNumber.foreground": "#eeee00",
    "editorLineNumber.activeForeground": "#eeee00",
    "editorCursor.foreground": "#f0e68c",
    "editor.selectionBackground": "#6b8e24",
    "editor.inactiveSelectionBackground": "#6b8e2488",
    "editor.selectionHighlightBackground": "#7f7f8c66",
    "editor.wordHighlightBackground": "#7f7f8c33",
    "editor.wordHighlightStrongBackground": "#bdb76b33",
    "editorBracketMatch.background": "#bdb76b",
    "editorBracketMatch.border": "#7f7f8c",
    "editorWhitespace.foreground": "#4d4d4d",
    "editorIndentGuide.background": "#4d4d4d",
    "editorIndentGuide.activeBackground": "#666666",

    "editorError.foreground": "#dd0000",
    "editorWarning.foreground": "#eeee00",
    "editorInfo.foreground": "#75a0ff",

    "diffEditor.insertedTextBackground": "#5f875f55",
    "diffEditor.removedTextBackground": "#af5faf55",
    "diffEditor.diagonalFill": "#5f87af55",

    "editorGutter.addedBackground": "#5f875f",
    "editorGutter.modifiedBackground": "#5f87af",
    "editorGutter.deletedBackground": "#af5faf",

    "list.focusBackground": "#666666",
    "list.activeSelectionBackground": "#f0e68c",
    "list.activeSelectionForeground": "#333333",
    "list.inactiveSelectionBackground": "#666666",
    "list.hoverBackground": "#4d4d4d",
    "list.hoverForeground": "#ffffff",
    "list.highlightForeground": "#f6ec96",

    "dropdown.background": "#666666",
    "dropdown.foreground": "#ffffff",
    "dropdown.listBackground": "#666666",

    "input.background": "#4d4d4d",
    "input.foreground": "#ffffff",
    "input.border": "#666666",

    "sideBar.background": "#424242",
    "sideBar.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#424242",
    "sideBarSectionHeader.foreground": "#eeee00",

    /* Activity Bar: higher contrast */
    "activityBar.background": "#3c3c3c",
    "activityBar.foreground": "#f6ec96",
    "activityBar.inactiveForeground": "#c2bfa5",
    "activityBarBadge.background": "#f6ec96",
    "activityBarBadge.foreground": "#1a1a1a",

    /* Title Bar: high readability */
    "titleBar.activeBackground": "#c2bfa5",
    "titleBar.activeForeground": "#1a1a1a",
    "titleBar.inactiveBackground": "#a8a58c",
    "titleBar.inactiveForeground": "#333333",

    "statusBar.background": "#c2bfa5",
    "statusBar.foreground": "#333333",
    "statusBar.noFolderBackground": "#c2bfa5",
    "statusBar.noFolderForeground": "#333333",
    "statusBar.debuggingBackground": "#c2bfa5",
    "statusBar.debuggingForeground": "#333333",

    /* Tabs header: darker stage for tabs */
    "editorGroupHeader.tabsBackground": "#2f2f2f",
    "editorGroupHeader.tabsBorder": "#00000000",

    /* Tabs: strong differentiation */
    "tab.activeBackground": "#f6ec96",
    "tab.activeForeground": "#1a1a1a",
    "tab.activeBorderTop": "#ffde9b",
    "tab.activeModifiedBorder": "#cd5c5c",

    "tab.inactiveBackground": "#3e3e3e",
    "tab.inactiveForeground": "#e6e2c0",

    "tab.hoverBackground": "#4a4a4a",
    "tab.hoverForeground": "#ffffff",

    "tab.unfocusedActiveBackground": "#e9dfa0",
    "tab.unfocusedActiveForeground": "#222222",
    "tab.unfocusedInactiveForeground": "#d6d2b0",

    "tab.border": "#00000000",
    "editorGroup.border": "#1f1f1f",

    "panel.background": "#4d4d4d",
    "panel.border": "#c2bfa5",

    "peekView.border": "#bdb76b",
    "peekViewEditor.background": "#4d4d4d",
    "peekViewResult.background": "#4d4d4d",

    "editorWidget.background": "#666666",
    "editorSuggestWidget.background": "#666666",
    "editorSuggestWidget.foreground": "#ffffff",
    "editorSuggestWidget.selectedBackground": "#f0e68c",
    "editorSuggestWidget.selectedForeground": "#333333",
    "editorSuggestWidget.highlightForeground": "#ffa0a0",
    "editorHoverWidget.background": "#666666",

    "notificationCenterHeader.background": "#666666",
    "notifications.background": "#666666",
    "notifications.foreground": "#ffffff",

    "terminal.background": "#333333",
    "terminal.foreground": "#ffffff",
    "terminalCursor.foreground": "#f0e68c",

    "terminal.ansiBlack":   "#7f7f8c",
    "terminal.ansiRed":     "#cd5c5c",
    "terminal.ansiGreen":   "#9acd32",
    "terminal.ansiYellow":  "#bdb76b",
    "terminal.ansiBlue":    "#75a0ff",
    "terminal.ansiMagenta": "#eeee00",
    "terminal.ansiCyan":    "#cd853f",
    "terminal.ansiWhite":   "#666666",
    "terminal.ansiBrightBlack":   "#8a7f7f",
    "terminal.ansiBrightRed":     "#ff0000",
    "terminal.ansiBrightGreen":   "#89fb98",
    "terminal.ansiBrightYellow":  "#f0e68c",
    "terminal.ansiBrightBlue":    "#6dceeb",
    "terminal.ansiBrightMagenta": "#ffde9b",
    "terminal.ansiBrightCyan":    "#ffa0a0",
    "terminal.ansiBrightWhite":   "#c2bfa5",

    "quickInput.background": "#4d4d4d",
    "quickInput.foreground": "#ffffff"
  },
  "tokenColors": [
    {
      "name": "Comment (Comment)",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6dceeb" }
    },
    {
      "name": "Strings (Constant)",
      "scope": ["string", "constant.character", "constant.other.symbol"],
      "settings": { "foreground": "#ffa0a0" }
    },
    {
      "name": "Numbers (Constant/Number)",
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": { "foreground": "#ffa0a0" }
    },
    {
      "name": "Keywords / Statements (Statement)",
      "scope": ["keyword", "keyword.control", "keyword.operator", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#f0e68c" }
    },
    {
      "name": "Preprocessor / Imports (PreProc)",
      "scope": ["meta.preprocessor", "keyword.control.import", "entity.other.attribute-name"],
      "settings": { "foreground": "#cd5c5c" }
    },
    {
      "name": "Types (Type)",
      "scope": ["entity.name.type", "support.type", "support.class", "storage.type.class", "storage.type.interface"],
      "settings": { "foreground": "#bdb76b" }
    },
    {
      "name": "Functions & Methods (Identifier/Function)",
      "scope": ["entity.name.function", "support.function", "meta.function-call", "variable.function"],
      "settings": { "foreground": "#89fb98" }
    },
    {
      "name": "Variables / Identifiers",
      "scope": ["variable", "meta.definition.variable", "support.variable", "meta.object-literal.key"],
      "settings": { "foreground": "#ffffff" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      /* "settings": { "foreground": "#89fb98" } */
      "settings": { "foreground": "#ffffff" }
    },
    {
      "name": "Constants & Special (Special)",
      "scope": ["constant.other", "constant.language", "support.constant"],
      "settings": { "foreground": "#ffde9b" }
    },
    {
      "name": "Classes / Types (Title)",
      "scope": ["entity.name.class", "entity.other.inherited-class", "entity.name.namespace"],
      "settings": { "foreground": "#cd5c5c" }
    },
    {
      "name": "Attributes / Annotations",
      "scope": ["meta.annotation", "storage.type.annotation"],
      "settings": { "foreground": "#cd5c5c" }
    },
    {
      "name": "Punctuation & Delimiters",
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#ffffff" }
    },
    {
      "name": "Links (Underlined)",
      "scope": ["markup.underline.link", "constant.other.reference.link"],
      "settings": { "foreground": "#75a0ff", "fontStyle": "underline" }
    },
    {
      "name": "Markdown Headings (Title)",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#cd5c5c" }
    },
    {
      "name": "Markup Emphasis (no bold)",
      "scope": ["markup.bold"],
      "settings": { "fontStyle": "" }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Search Match (Search)",
      "scope": ["meta.find-match"],
      "settings": { "background": "#7f7f8c", "foreground": "#f0e68c" }
    },
    {
      "name": "Invalid / Error (Error)",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff0000", "background": "#ffffff" }
    },
    {
      "name": "Debug/Log (debugBreakpoint)",
      "scope": ["meta.debug", "variable.language.this"],
      "settings": { "foreground": "#ffa0a0" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "comment": { "foreground": "#6dceeb" },
    "keyword": { "foreground": "#f0e68c", "bold": true },
    "string": { "foreground": "#ffa0a0" },
    "number": { "foreground": "#ffa0a0" },
    "function": { "foreground": "#89fb98" },
    "method": { "foreground": "#89fb98" },
    "parameter": { "foreground": "#ffffff" },
    "variable": { "foreground": "#ffffff" },
    "namespace": { "foreground": "#ffffff" },
    "module": { "foreground": "#ffffff" },
    "property": { "foreground": "#ffffff" },
    "enumMember": { "foreground": "#ffde9b" },
    "type": { "foreground": "#bdb76b", "bold": true },
    "class": { "foreground": "#cd5c5c", "bold": true }
  }
}
